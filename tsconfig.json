{
  "extends": "@uwu-codes/tsconfig",
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": "./src",
    "strictPropertyInitialization": false,
    "inlineSourceMap": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "module": "ESNext",
    "skipLibCheck": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "build"]
}
